{
  "name": "launch-functions",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Turn JS functions into command line tools and web UIs",
  "authors": [
    "Andre Landgraf <andre.timo.landgraf@gmail.com>"
  ],
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./types": "./dist/types.d.ts"
  },
  "main": "dist/index.js",
  "scripts": {
    "fmt": "prettier --write .",
    "typecheck": "tsc --noEmit",
